  "keywords": [
    "openai",
    "realtime-api",
    "webrtc",
    "websocket",
    "voice-chat",
    "ai-assistant",
    "gpt-4o",
    "gpt-4o-realtime-preview-2024-12-17",
    "multimodal",
    "transcription",
    "semantic-vad",
    "server-vad",
    "function-calling",
    "emotion-analysis",
    "ai-facial-expressions",
    "dall-e-3",
    "contextual-image-generation",
    "noise-reduction",
    "confidence-scores",
    "log-probabilities",
    "interrupt-handling",
    "beta-api"
  ],
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "multer": "^2.0.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/realtime-api-backend.git"
  },
  "author": "Your Name",
  "license": "MIT",
  "features": {
    "realtime_models": ["gpt-4o-realtime-preview-2024-12-17"],
    "transcription_models": ["gpt-4o-transcribe", "gpt-4o-mini-transcribe", "whisper-1"],
    "image_models": ["dall-e-3"],
    "connection_methods": ["webrtc", "websocket"],
    "vad_types": ["semantic_vad", "server_vad"],

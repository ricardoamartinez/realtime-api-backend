    "noise_reduction": ["near_field", "far_field"],
    "enhanced_features": [
      "semantic_vad_with_eagerness_control",
      "real_time_function_calling",
      "ai_facial_expression_control",
      "contextual_image_generation",
      "enhanced_error_handling",
      "confidence_scores_and_logprobs",
      "interrupt_handling",
      "beta_api_support"
    ]
  }
